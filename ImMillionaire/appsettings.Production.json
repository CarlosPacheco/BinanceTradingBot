{
  "Config": {
    "ApiKey": "your-key-here",
    "SecretKey": "your-secret-here"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Trace",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning",
        "CryptoExchange.Net": "Warning",
        "Binance.Net": "Warning"
      }
    }
  }
}
